/********************弹窗相关属性****************************/

/*************************遮罩******************************/
.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 9000;
    opacity: 0.6;
}
 
 /***************************弹窗***************************/
.modalDlg{
    width: 80%;
    height: 540rpx;
    position: fixed;
    top: 50%;
    left: 0;
    z-index: 9999;
    margin: -370rpx 85rpx;
    background-color: #fff;
    border-radius: 50rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

/***************************退出弹窗**********************************/
.close_mask {
  color: rgb(255, 50, 50);
  background-color: #0000;
  position: relative;
  left: 40%;
  /* top: -82%; */
  font-size: 40rpx;
}
.close_mask::after{
  border: 0;
}

